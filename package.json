    "jest": "^28.0.3",
    "muuri": "^0.9.5",
    "prettier": "^2.6.2",
    "rollup": "2.71.1",
    "ts-jest": "^27.1.4",
    "tslib": "^2.4.0",
    "typescript": "^4.6.4"
